/*
***
Sample Input1:
5

6
9
12
8
17
output:
1(5) 2(3) 3(3) 4(2) 6(2) 8(1) 9(1) 12(1) 17(1)


SampleINput2:

9

79
27
37
19
63
13
23
61
5

Sample Output2:

1(9) 3(2) 9(2) 5(1) 7(1) 13(1) 19(1) 21(1) 23(1) 27(1) 37(1) 61(1) 63(1) 79(1)

*
**
*/



import java.util.*;
public class Fc{
    public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt(),i,j,count=0;
		int a[]=new int[n];
		List<Integer> l=new ArrayList<Integer>();
		Set<Integer> k=new LinkedHashSet<Integer>();
		for(i=0;i<n;i++){
		    count=0;
		    a[i]=s.nextInt();
		    for(j=1;j<=a[i];j++){
		        if(a[i]%j==0){
		            l.add(j);
		            k.add(j);
		            count++;
		        }
		    }
		}
		j=0;
		int ar[]=new int[k.size()];
		int arr[]=new int[k.size()];
		for(int val:k){
		    arr[j]=val;
		    j++;
		}
		j=0;
		for(int val:k){
		    count=0;
		    for(i=0;i<l.size();i++){
		        if(val==l.get(i)){
		            count++;
		        }
		    }
		    ar[j]=count;
		    j++;
		}
		int t1,t2;
		for(i=0;i<ar.length;i++){
		    for(j=i+1;j<ar.length;j++){
		        if(ar[i]==ar[j]){
		            if(arr[i]>arr[j]){
		                t1=arr[i];
		                arr[i]=arr[j];
		                arr[j]=t1;
		            }
		        }
		        if(arr[i]>arr[i+1]){
                    t1=ar[i];
                    ar[i]=ar[j];
                    ar[j]=t1;
                    t2=arr[i];
                    arr[i]=arr[j];
                    arr[j]=t2;
		        }
		    }
		}
		for(i=0;i<ar.length;i++){
		    System.out.print(arr[i]+"("+ar[i]+")"+" ");
		}
	}
}
